{
  "name": "react-ui5",
  "version": "0.1.7",
  "description": "A react based implementation of the SAP OpenUI5 framework",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component --copy-files",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react-router": "^3.0.0"
  },
  "peerDependencies": {
    "react": "15.x"
  },
  "devDependencies": {
    "nwb": "^0.14.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "author": "Alessandro Holanda",
  "homepage": "http://www.github.com/alessh",
  "license": "MIT",
  "repository": "http://www.github.com/alessh/react-ui5.git",
  "keywords": [
    "react",
    "react-component",
    "OpenUI5",
    "SAP"
  ]
}
